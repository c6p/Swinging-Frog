{
  "name": "simple-phaser3-scaffold",
  "version": "1.0.0",
  "description": "A blank Koji Phaser 3 template with webpack bundler",
  "author": "snowbillr",
  "scripts": {
    "build": "NODE_ENV=production webpack --config ./.internals/webpack.production.js --color -p --progress --hide-modules --display-optimization-bailout",
    "prestart": "koji-vcc &",
    "start": "webpack-dev-server --config ./.internals/webpack.development.js --hot --inline"
  },
  "keywords": [
    "Koji",
    "Webpack",
    "Phaser",
    "Phaser 3"
  ],
  "dependencies": {
    "@withkoji/vcc": "^1.1.4",
    "phaser": "^3.22.0",
    "phaser-matter-collision-plugin": "^0.10.2",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.4.2",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "style-loader": "^0.23.1",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
